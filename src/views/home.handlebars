<div class="homePage">
    <table id="tableHome" class="table table-lg align-middle">
        <thead class="table-light">
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Description</th>
                <th>Code</th>
                <th>Price</th>
                <th>Status</th>
                <th>Stock</th>
                <th>Category</th>
                <th>Tumbnail</th>
                <th>__v</th>
            </tr>  
        </thead>
        <tbody>
            {{#each productsList}}
                <tr>
                    {{#each _doc}}
                        <td>{{this}}</td>
                    {{/each}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

