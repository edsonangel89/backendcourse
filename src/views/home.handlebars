<h1>HOLA {{name}}</h1>
<nav class="navbar bg-body-tertiary">
  <form id="close" action="http://localhost:8080/api/sessions/logout" method="get" class="container-fluid justify-content-end">
    <button class="btn btn-outline-light me-2" type="submit">Cerrar sesion</button>
  </form>
</nav>
<div class="homePage">
    <table id="tableHome" class="table table-lg align-middle">
        <thead class="table-light">
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Description</th>
                <th>Code</th>
                <th>Price</th>
                <th>Status</th>
                <th>Stock</th>
                <th>Category</th>
                <th>Tumbnail</th>
                <th>__v</th>
            </tr>  
        </thead>
        <tbody>
            {{#each productsList}}
                <tr>
                    {{#each _doc}}
                        <td>{{this}}</td>
                    {{/each}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

