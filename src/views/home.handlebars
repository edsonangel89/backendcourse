<table id="tableHome">
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Code</th>
        <th>Price</th>
        <th>Status</th>
        <th>Stock</th>
        <th>Category</th>
        <th>Tumbnail</th>
        <th>V</th>
    </tr>  
    {{#each product}}
        <tr>
            <td>{{_id}}</td> 
            <td>{{title}}</td> 
            <td>{{description}}</td> 
            <td>{{code}}</td> 
            <td>{{price}}</td> 
            <td>{{status}}</td> 
            <td>{{stock}}</td> 
            <td>{{category}}</td> 
            <td>{{thumbnail}}</td> 
            <td>{{__v}}</td> 
        </tr>
    {{/each}}
    {{#each products}}
        <tr>
            {{#each _doc}}
                <td>{{this}}</td> 
            {{/each}}
        </tr>
    {{/each}}
    {{#each prods}}
        <tr>
            <td>{{id}}</td> 
            <td>{{title}}</td> 
            <td>{{description}}</td> 
            <td>{{code}}</td> 
            <td>{{price}}</td> 
            <td>{{status}}</td> 
            <td>{{stock}}</td> 
            <td>{{category}}</td> 
            <td>{{thumbnail}}</td> 
        </tr>
    {{/each}}
</table>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/index.js"></script>
