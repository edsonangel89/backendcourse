<div class="homePage">
    <table id="tableHome" class="table table-lg align-middle">
        <thead class="table-light">
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Description</th>
                <th>Code</th>
                <th>Price</th>
                <th>Status</th>
                <th>Stock</th>
                <th>Category</th>
                <th>Tumbnail</th>
            </tr>  
        </thead>
        <tbody>
            {{#each prods}}
                <tr>
                    <td>{{id}}</td> 
                    <td>{{title}}</td> 
                    <td>{{description}}</td> 
                    <td>{{code}}</td> 
                    <td>{{price}}</td> 
                    <td>{{status}}</td> 
                    <td>{{stock}}</td> 
                    <td>{{category}}</td> 
                    <td>{{thumbnail}}</td> 
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

